    "body": [
      "function throttle(func, wait, immediate) {",
      "  let timeout, lastTime = 0;",
      "",
      "  return function(...args) {",
      "    const now = Date.now();",
      "",
      "    if (immediate && now - lastTime >= wait) {",
      "      func.apply(this, args);",
      "      lastTime = now;",
      "    } else {",
      "      clearTimeout(timeout);",
      "      timeout = setTimeout(() => {",
      "        if (!immediate) {",
      "          func.apply(this, args);",
      "        }",
      "        lastTime = Date.now();",
      "      }, wait);",
      "    }",
      "  };",
      "}"
    ],
    "description": "节流函数",
    "create_date": "2025/1/15"
